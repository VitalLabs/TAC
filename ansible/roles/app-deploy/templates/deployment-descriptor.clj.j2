
 {:root "/var/lib/immutant/current/archives/{{ switchboard_app_name }}{{ switchboard_app_root }}/target{{ switchboard_app_root }}.ima"
 :nrepl-port 4005
 :context-path "/{{ switchboard_context }}" 
 :virtual-host ["{{ inventory_hostname }}" "{{ orchestra_vhost }}" "{{ ansible_default_ipv4.address }}"]
 :datomic.template "{{ datomic_template }}"

 :switchboard.api "{{ switchboard_twillio_callback_url }}"
  

 :twilio.enable "false"
 :twilio.sid "{{ twilio_sid }}"
 :twilio.token "{{ twilio_token }}"



 :service.smtp/host "smtp.sendgrid.net"
 :service.smtp/ssl true
 :service.smtp/enable true
 :service.smtp/user "{{ service_smtp_user }}"
 :service.smtp/pass "{{ service_smtp_pass }}"



 :service.push/app-id "{{ push_app_id }}"
 :service.push/rest-id "{{ push_rest_id }}"

 :datastax.hosts [{% for host in groups['tag_'+environ|default('dev')+'_cassandra_'] %}"{{ hostvars[host]['ec2_private_ip_address'] }}"{% if not loop.last %}, {% endif %}{% endfor %}]
 :datastax.port {{cassandra_native_port}}
 :datastax.rep.class "{{ cassandra_rep_class }}"
 :datastax.rep.factor.name "{{ cassandra_rep_factor_name }}"
 :datastax.rep.factor.count {{ cassandra_rep_factor_count }}

:lein-profiles [:{{ switchboard_lein_profiles }}]}
