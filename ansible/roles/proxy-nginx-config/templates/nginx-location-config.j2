		location ^~ /api/ {
			proxy_connect_timeout   10;
			proxy_send_timeout      {{ nginx_send_timeout }};
			proxy_read_timeout      {{ nginx_send_timeout }};
			proxy_next_upstream error timeout http_404;
			proxy_set_header X-Forwarded-Host $host;
			proxy_set_header X-Forwarded-Server $host;
			proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for; 
      add_header 'Access-Control-Allow-Methods' 'GET,POST,DELETE,OPTIONS';
			add_header 'Access-Control-Allow-Headers' 'Set-Cookie, Accept';
			add_header 'Access-Control-Expose-Headers' 'Set-Cookie, Accept';

			{% if proxy_allow_origin_header %}
		   add_header 'Access-Control-Allow-Origin' "$http_origin";
      {% endif %}

			add_header 'Pragma' 'nocache';
			add_header 'Cache-Control' 'o-cache';
			add_header 'Expires' '0';
			proxy_cache off;
			proxy_set_header Host {{ orchestra_vhost }};
			proxy_set_header X-Real-IP $remote_addr;
			proxy_pass http{{ proxy_pass_ssl }}://SwitchBoard;
		}

		location ^~ /service/ {
			proxy_connect_timeout   10;
			proxy_send_timeout      {{ nginx_send_timeout }};
			proxy_read_timeout      {{ nginx_send_timeout }};
			proxy_next_upstream error timeout http_404;
			proxy_set_header X-Forwarded-Host $host;
			proxy_set_header X-Forwarded-Server $host;
			proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for; 
      add_header 'Access-Control-Allow-Methods' 'GET,POST,DELETE,OPTIONS';
			add_header 'Access-Control-Allow-Headers' 'Set-Cookie, Accept';
			add_header 'Access-Control-Expose-Headers' 'Set-Cookie, Accept';

      {% if proxy_allow_origin_header %}
		   add_header 'Access-Control-Allow-Origin' "$http_origin";
      {% endif %}
      

			add_header 'Pragma' 'nocache';
			add_header 'Cache-Control' 'o-cache';
			add_header 'Expires' '0';
			proxy_cache off;
			proxy_set_header Host {{ orchestra_vhost }};
			proxy_set_header X-Real-IP $remote_addr;
			proxy_pass http{{ proxy_pass_ssl }}://SwitchBoard;
		}


		location ^~ /app-downloads {
			
			  rewrite ^/app-downloads/ios/index.html https://s3.amazonaws.com/vitalreactor-ios/index.html;
			  rewrite ^/app-downloads/android/index.html https://s3.amazonaws.com/vitalreactor-android/index.html;
		} 